<h1>Listing players</h1>
:>>>
<table>
  <tr>
	<th>Id</th>
	<th>FB</th>
	<th>devices</th>
    <th>Nickname</th>
	<th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Num Wins</th>
	<th>Num Visits</th>
	<th>digest</th>
	<th>password</th>
  </tr>

<% @players.each do |player| %>
  <tr>
    <td><%= player.id %></td> 
    <td><%= player.fb %></td>
	<td><%= player.devices %></td>
    <td><%= player.n_n %></td>
	<td><%= player.f_n %></td>
	<td><%= player.l_n %></td>
	<td><%= player.e_m %></td>
	<td><%= player.n_w %></td>
	<td><%= player.n_v %></td>
	<td><%= player.password_digest %></td>
	<td><%= player.password %></td>
  </tr>
<% end %>
</table>

<br />
