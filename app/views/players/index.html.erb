<h1>Listing players</h1>
:>>>
<table>
  <tr>
	<th>Id</th>
	<th>FB</th>
	<th>auth_token</th>
    <th>Nickname</th>
	<th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Num Wins</th>
	<th>digest</th>
  </tr>

<% @players.each do |player| %>
  <tr>
    <td><%= player.id %></td>
    <td><%= player.fb %></td>
	<td><%= player.auth_token %></td>
    <td><%= player.n_name %></td>
	<td><%= player.f_name %></td>
	<td><%= player.l_name %></td>
	<td><%= player.email %></td>
	<td><%= player.n_w %></td>
	<td><%= player.password_digest %></td>
  </tr>
<% end %>
</table>

<br />
