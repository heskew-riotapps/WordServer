<h1>Listing games</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Status</th>
	<th>Turn</th>
  </tr>

<% @games.each do |game| %>
  <tr>
    <td><%= game.id  %></td>
	  <td><%= game.st  %></td>
	   <td><%= game.t  %></td>
	   <td><%= game.l_t_a %></td>
	    <td><%= game.l_t_d %></td>
		 <td><%= game.l_t_p %></td>
  </tr>
<% end %>
</table>

<br />
